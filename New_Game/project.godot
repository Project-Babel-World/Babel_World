config_version=5

[application]

config/name="Babel World"
run/main_scene="res://Scenes(Prefabs)/rest_scene.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
config/icon="res://icon.svg"

[autoload]

SignalBus="*res://singletons/SignalBus.gd"
Checkpoint="*res://Checkpoint.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
DialgoueState="*res://dialog/dialgoue_state.gd"

[dialogue_manager]

general/states=["DialgoueState"]

[display]

window/size/viewport_width=2000
window/size/viewport_height=900
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

